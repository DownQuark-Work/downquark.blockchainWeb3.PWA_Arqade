{
  "name": "@lit/lit-starter-ts",
  "private": true,
  "version": "1.0.0",
  "description": "A simple web component",
  "main": "my-element.js",
  "module": "my-element.js",
  "scripts": {
    "build-storybook": "yarn cln && yarn tkn && build-storybook",
    "build:tsc": "tsc",
    "build:watch": "tsc --watch",
    "build": "yarn build-storybook",
    "dev": "yarn storybook",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "storybook:debug": "yarn storybook --debug-webpack",
    "storybook:tkn": "yarn tkn && start-storybook -p 6006",
    "storybook": "start-storybook -p 6006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tkn": "style-dictionary build -c \"./utils/token/token-config.js\""
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Google LLC",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@dan-nz/storybook-documentation-primitives": "file:./utils/token/@dan-nz/storybook-documentation-primitives",
    "lit": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/web-components": "^6.4.9",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.12",
    "@typescript-eslint/eslint-plugin": "^4.3.0",
    "@typescript-eslint/parser": "^4.30.0",
    "@webcomponents/webcomponentsjs": "^2.6.0",
    "babel-loader": "^8.2.3",
    "deepmerge": "^4.2.2",
    "eslint": "^7.32.0",
    "lit-analyzer": "^1.1.10",
    "mocha": "^9.1.1",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.5"
  }
}
